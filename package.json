{
  "name": "flux",
  "displayName": "flux",
  "description": "word highlighting",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "flux",
      "aliases": ["Flux", "flux"],
      "extensions": [".flux"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "flux",
      "scopeName": "source.flux",
      "path": "./syntaxes/flux.tmLanguage.json"
    }]
  }
}
